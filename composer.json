{
    "description": "This repository contains GroupDocs.Comparison for Cloud SDK for PHP source code.",
    "name": "groupdocscloud/comparison-sdk-php",
    "version": "18.4.1",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "groupdocs",
        "comparison",
        "cloud",
        "api",
        "php",
        "sdk"
    ],
    "homepage": "https://github.com/groupdocs-comparison-cloud/groupdocs-comparison-cloud-php",
    "authors": [
        {
            "name": "GroupDocs",
            "homepage": "https://github.com/groupdocs-comparison-cloud"
        }
    ],
    "config": {
        "platform": {
            "php": "5.6.32"
        }
    },
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",        
        "phpunit/phpunit": "4.7"
    },
    "require-dev": {
        "aspose/storage-sdk-php": "*",
        "friendsofphp/php-cs-fixer": "2.10.*",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "classmap": [ "src/" ],
        "psr-4": { "GroupDocs\\" : "src/" }
    },
    "autoload-dev": {
        "classmap": [ "src/" ],
        "psr-4": { "GroupDocs\\" : "tests/" }
    }
}
